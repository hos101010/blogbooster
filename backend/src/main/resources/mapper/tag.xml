<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.snl.blogbooster.mapper.TagMapper">
    <insert id="insertTag" parameterType="com.snl.blogbooster.model.domain.Tag">
        insert into Tag
        (
        item_num,
        item_type,
        content,
        audit_at)
        values (
        #{itemNum},
        #{itemType},
        #{content},
        DATE_FORMAT(now(), '%Y%m%d')
        )
    </insert>
</mapper>